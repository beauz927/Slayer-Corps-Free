local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
 
local Window = Rayfield:CreateWindow({
	Name = "Slayer Corps Hub BETA",
	LoadingTitle = "Slayer Corps DemonFall Hub BETA",
	LoadingSubtitle = "By InfinitysWolrd",
	ConfigurationSaving = {
		Enabled = false,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Slayer Corps Hub BETA"
	},
	Discord = {
		Enabled = true,
		Invite = "slayercorps", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
		RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = false, -- Set this to true to use our key system
	KeySettings = {
		Title = "Slayer Corps Premium Hub Key",
		Subtitle = "Premium Hub Key System",
		Note = "Thanks For Buying The Premium Key. Enjoy!",
		FileName = "Slayer Corps Premium Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
		SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
		GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = {"FishPairTickleGingerJimHairballDiscordDemonFallDoingThisForYouLotWasFunEnjoyTheScriptPlease"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
	}
})
 
local MainTab = Window:CreateTab("Main", 4483362458) -- Title, Image
local MainSection = MainTab:CreateSection("")
 
Rayfield:Notify({
	Title = "Premium Script Executed Succesfully",
	Content = "Enjoy My Friend!",
	Duration = 4,
	Image = 4483362458,
	Actions = { -- Notification Buttons
		Ignore = {
			Name = "Okay!",
			Callback = function()
				print("The user tapped Okay!")
			end
		},
	},
})
 
local MainSection = MainTab:CreateSection("Infinite Sta/Hp")
local Button = MainTab:CreateButton({
	Name = "God Mode",
	Callback = function()
		local args = {
			[1] = "Character",
			[2] = "FallDamageServer",
			[3] = 0/0 --[[NaN]]
		}
 
		game:GetService("ReplicatedStorage").Remotes.Async:FireServer(unpack(args))
	end,
})
 
 local MainToggle = MainTab:CreateToggle({
    Name = "Auto Equip Sword NOT WORKING RIGHT NOW",
    CurrentValue = false,
    Flag = "Toggle4", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
    _G.AutoSword = true 
    end,
 })
 
